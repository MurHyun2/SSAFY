package com.ssafy.off.day06.board;

public class BoardManager {
	final int MAX_SIZE = 10;
	Board[] boardList = new Board[MAX_SIZE];
	int size = 0;

	boolean add(Board board) {
		if (size == MAX_SIZE)
			return false;
		boardList[size++] = board;
		return true;
	}

	Board[] getBoards() {
		return null;
	}

	boolean modify() {
		return true;
	}

	boolean delete() {
		return true;
	}

}
